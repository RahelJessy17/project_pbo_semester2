@model tabungan_project.Models.category

@{
    ViewBag.Title = "Delete";
}

<h2 style="color: #FFC300"><b>CATEGORI</b></h2>
<h4 style="color: #FFC300">Hapus Categori</h4>
<hr />
<h5>Apakah kamu yakin ingin menghapus categori ini?</h5>

<div>
    <br />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.jenis_kebutuhan)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.jenis_kebutuhan)
        </dd>

    </dl>
    <br />
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="form-actions no-color">
            <div>
                <input type="submit" value="Delete" class="btn btn-danger" /> 
                @Html.ActionLink("List", "Index", new { id = Model.id_category }, new { @class = "btn btn-info" })
            </div>
        </div>
    }
</div>
